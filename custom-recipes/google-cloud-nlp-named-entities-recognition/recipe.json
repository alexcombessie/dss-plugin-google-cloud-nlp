{
    "meta": {
        "label": "Named Entity Recognition",
        "description": "Recognize “real-world objects” (people names, dates, places, etc.) in a text",
        "icon": "icon-tags"
    },
    "kind": "PYTHON",
    "selectableFromDataset": "input_dataset",
    "inputRoles": [
        {
            "name": "input_dataset",
            "label": "Input Dataset",
            "description": "Dataset containing the text data to analyze",
            "arity": "UNARY",
            "required": true,
            "acceptsDataset": true
        }
    ],
    "outputRoles": [
        {
            "name": "output_dataset",
            "label": "Output dataset",
            "description": "Dataset with enriched output",
            "arity": "UNARY",
            "required": true,
            "acceptsDataset": true
        }
    ],
    "params": [
        {
            "name": "connection_info",
            "label": "GCP Connection",
            "type": "PRESET",
            "parameterSetId": "connection-info",
            "mandatory": true
        },
        {
            "name": "text_column",
            "label": "Input text column",
            "type": "COLUMN",
            "columnRole": "input_dataset",
            "mandatory": true
        },
        {
            "name": "language",
            "label": "Language",
            "type": "COLUMN",
            "columnRole": "input_dataset",
            "description": "in ISO 639-1 format (empty = auto-detect)",
            "mandatory": false
        },
        {
            "name": "output_format",
            "label": "Output Format",
            "type": "SELECT",
            "selectChoices": [
                {
                    "value": "single_column",
                    "label": "A single JSON column"
                },
                {
                    "value": "multiple_columns",
                    "label": "One colum per entity type"
                }
            ],
            "defaultValue": "single_column",
            "mandatory": true
        },
        {
            "name": "should_output_raw_results",
            "label": "Output Raw Response?",
            "type": "BOOLEAN"
        }
    ],
    "resourceKeys": []
}